.travel-main-part {
    margin: 0 30px;
    padding-top: 40px;
}
.travel-problem-account {
    display: block;
    overflow: hidden;
    margin-bottom: 5px;
    position: relative;
}
.travel-input-container {
    border-bottom: 1px solid #e2e2e2;
    display: flex;
}
.travel-input-container text.fL {
    font-size: 14px;
    line-height: 34px;
    width: 80px;
    text-align: left;
    color: #333;
    font-weight: bold;
    vertical-align: middle;
}
.travel-main-part input {
    position: relative;
    border: none;
    border-radius: 0;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    -webkit-appearance: none;
    z-index: 10;
    background: transparent;
    color: #999999;
    height: 34px;
    font-size: 14px;
    -webkit-box-flex: 1;
    -ms-flex: 1;
    flex: 1;
    zoom: 1;
}
.travel-problem-account .travel-prompt {
    height: 26px;
    line-height: 26px;
    font-size: 12px;
    color: #999999;
    visibility: hidden;
}
.travel-button {
    margin: 55px auto 0;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    text-align: center;
}
.travel-button button{
    -webkit-box-flex: 1;
    -ms-flex: 1;
    flex: 1;
    position: relative;
    zoom: 1.1;
    display: block;
    background: #3bc983;
    border-radius: 25px;
    color: white;
    font-size: 14px;
    line-height: 31px;
    transition: all .45s;
}