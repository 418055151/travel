.img-container {
    background: url(https://www.mxlgsl.cn/static/images/wechat/Rectangle6.png)no-repeat;
    background-size: 100% 100%;
    padding-bottom: 60rpx;
}

.container {
    display: block;
    width: 202rpx;
    padding: 30rpx 0 0;
    text-align: center;
    margin: 0 auto;
    color: white;
    font-size: 32rpx;
}

.mine-img {
    width: 164rpx;
    height: 164rpx;
    overflow: hidden;
    border-radius: 50%;
    background: white;
    display: inline-block;
    padding: 6rpx;
}

.mine-img image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.nickName {
    width: 202rpx;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.container div {
    font-size: 28rpx;
    font-weight: bold;
    font-family: poppin, Tahoma, Arial, \5FAE\8F6F\96C5\9ED1, sans-serif;
}

.list {
    margin-top: 50rpx;
    background-color: #fff;
    margin-bottom: 20rpx;
}

.list-item {
    position: relative;
    margin-left: 90rpx;
    margin-right: 40rpx;
    height: 90rpx;
    line-height: 90rpx;
    border-bottom: 2rpx solid #ddd;
}

.list-item:nth-child(1) {
    border-top: 2rpx solid #ddd;
}

.list-item.arrow-right:before {
    position: absolute;
    top: 50%;
    right: 0;
    margin-top: -6rpx;
    border-width: 4rpx 4rpx 0 0;
    border-style: solid;
    width: 12rpx;
    height: 12rpx;
    color: #ccc;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    content: "";
}

.ServiceTel {
    display: block;
    vertical-align: middle;
    font-size: 24rpx;
    color: #333;
    position: absolute;
    top: 0;
    right: 50rpx;
}

.list-item-link {
    display: inline-block;
    color: #999;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
}

.icon {
    vertical-align: middle;
    color: #fff;
    width: 40rpx;
    height: 50rpx;
    display: inline-block;
    margin-left: -50rpx;
    margin-right: 10rpx;
}

.zhanghaoicon {
    background: url(https://www.mxlgsl.cn/static/images/wechat/zhanghaoicon.png) no-repeat;
    background-size: 40rpx 50rpx;
}

.wodedingdan {
    background: url(https://www.mxlgsl.cn/static/images/wechat/wodedingdan.png) no-repeat;
    background-size: 40rpx 50rpx;
}

.xiugaizhanghao {
    background: url(https://www.mxlgsl.cn/static/images/wechat/xiugaizhanghao.png) no-repeat;
    background-size: 40rpx 50rpx;
}

.list-item-name {
    vertical-align: middle;
    font-size: 24rpx;
    color: #333;
}
