<view class='MemberLoginBox'>
    <image class='logoimage' src='data:image/png;base64,'></image>
    <view class='MemberLoginBtnBox'>
        <button class='getPhoneNumber' open-type='getPhoneNumber'>手机号一键登录</button>
        <button class='getUserInfo' open-type='getUserInfo'>微信账号登录</button>
        <button class='MessageAuthenticationLogin' bindtap='MessageAuthenticationShow'>短信验证登录</button>
    </view>
</view>
<view class="search-mask">
    <view class="m-search-page {{MessageAuthenticationLogin?'':'z-open'}}">
        <view class="AuthenticationLogin">
            <view class="AuthenticationLogin_title">短信验证登录</view>
            <view class='warning_msg'>请输入手机号</view>
            <view class='PhoneNumberinput'>
                <input class="inputText" cursor-spacing='10' placeholder-style='color:#ddd;' name='PhoneNum' placeholder="注册或绑定手机号" type="number" bindinput="PhoneNum" />
            </view>
            <view class='MessageAuthenticationinput'>
                <input class="inputText" cursor-spacing='10' placeholder-style='color:#ddd;' name='PhoneNum' placeholder="短信动态码" type="number" bindinput="PhoneNum" />
                <view class='getMessageAuthentication'>获取动态码</view>
            </view>
            <view class='login_btn'>登    录</view>
        </view>
    </view>
</view>
<view class="mask m-search-mask {{MessageAuthenticationLogin?'mask-open':''}}" bindtouchstart="cancel"></view>